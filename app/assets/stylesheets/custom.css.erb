footer {
    padding-top: 60px;
    margin-top: 100px;
}

strike {
    font-size: 10px
}

body {
    padding-top: 41px;
}

.body-wrapper .container {
    box-sizing: border-box; 
    padding-left: 10px;
    padding-right: 10px;
    background: url("<%= asset_path("cream_dust.transparent.png") %>") repeat 0 0;
}

body, input, button, select, textarea {
    font-family: "Helvetica Neue", Helvetica, Arial, "微軟正黑體", "微软雅黑", "メイリオ", "맑은 고딕", sans-serif;
    font-size: 108%;
    letter-spacing: 2px;
}

h1,h2, h3, h4, h5, h6 {
    font-weight: normal;
}


